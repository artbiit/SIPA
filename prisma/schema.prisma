model Users {
  id           Int            @id @default(autoincrement())
  userId       String         @unique @db.VarChar(15)
  password     String         @db.VarChar(15)
  userName     String         @db.VarChar(16)
  cash         Int            @default(0)
  createdAt    DateTime       @default(now())
  MMR          MMR?
  MyTeam       MyTeam?
  UsersAthlete UsersAthlete[]
}

